import sys
from PyQt5.QtCore import *
from PyQt5.QtGui import *
from PyQt5.QtWidgets import *

class ConfigTable(QTabWidget):
    def __init__(self, parent=None):
        super(ConfigTable,self).__init__(parent)
        self.setWindowTitle("配置选项")

        self.tab1 = QWidget()
        self.tab2 = QWidget()
        self.tab3 = QWidget()

        self.addTab(self.tab1, 'TX_Config')
        self.addTab(self.tab2, 'RX_Config')
        self.addTab(self.tab3, 'RF_Config')


if __name__ == '__main__':
    app = QApplication(sys.argv)
    configWin = ConfigTable()
    configWin.show()
    sys.exit(app.exec_())